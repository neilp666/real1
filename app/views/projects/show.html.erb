<div class="container">
<div class="row">
  <div class="col-md-6">
    <div class="thumbnail">
      <%= image_tag "real.png", class: "img-responsive" %>
    </div>
  </div>

<div class="col-md-6">
  <h3><%= @project.name %></h3>
  <p><%= @project.description %></p>
  <p>Budget: <%= @project.budget %> Duration: <%= @project.duration %></p>
  <p>Organisation: <%= @project.organisation %></p>
</div>
</div>
</div>

<%= link_to 'Edit', edit_project_path(@project), class: "btn btn-link" %> |
<%= link_to 'Back', projects_path, class: "btn btn-link" %>
